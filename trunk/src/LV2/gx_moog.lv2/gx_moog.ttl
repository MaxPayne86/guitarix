@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix mod: <http://portalmod.com/ns/modgui#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix time: <http://lv2plug.in/ns/ext/time/#>.

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_moog_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_moog_" .

<http://guitarix.sourceforge.net/plugins/gx_moog_#_moog_>
    a lv2:Plugin ,
        lv2:FilterPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "Gxmoog";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_moog_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
      
    lv2:minorVersion 28;
    lv2:microVersion 3;

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "out1" ;
        lv2:name "Out1"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "in1" ;
        lv2:name "In1" ;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "Q" ;
        lv2:name "Q" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 4.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "FR" ;
        lv2:name "FR" ;
        lv2:default 3e+03 ;
        lv2:minimum 4.4e+02 ;
        lv2:maximum 6e+03 ;
        units:unit units:hz ;
    ];
    mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-gxmoog.html>;
        mod:templateData <modgui/data-gxmoog.json>;
        mod:screenshot <modgui/screenshot-gxmoog.png>;
        mod:thumbnail <modgui/thumb-gxmoog.png>;
    ] .
